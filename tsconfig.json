{
  "compilerOptions": {
    "target": "es2022",
    "module": "commonjs" ,

    "lib": ["ES2022"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
     "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
   
    
  },
  "exclude": [
    "node_modules",
  ],
  
  "include": [
    "./**/*.ts",    "./src/**/*.ts","src/",
]
}